{
    "server": {
        "accessLog": "/tmp/access.log",
        "workers": 10,
        "maxSockets": 100,
        "deadBackendTTL": 30,
        "tcpTimeout": 30,
        "retryOnError": 3,
        "deadBackendOn500": true,
        "httpKeepAlive": false
    },
    "http": {
        "port": 80,
        "bind": "0.0.0.0"
    },
    "driver": "redis://redis:6379"
}
